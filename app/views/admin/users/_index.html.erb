<div class="container-fluid">
	<div class="row">
		<div class="col-lg-12">
			<div class="page-header">
				<h1>
					<small><i class="fa fa-male"></i> Users</small>
					<div class="pull-right">
						<%= link_to "<i class='fa fa-plus'></i> New User".html_safe, new_admin_user_path, class: "btn btn-warning btn-sm" %>
					</div>
				</h1>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="col-lg-offset-1 col-lg-10">
			<table class="table table-striped table-hover">
				<tr class="warning">
					<th></th>
					<th>Email</th>
					<th>Email</th>
					<th>Actions</th>
				</tr>
				<% @users.each do |user| %>
					<tr>
						<td></td>
						<td><a href="#" onclick="get_click(<%= user.id %>);"><%= user.email %></a></td>
						<td><%= user.email %></td>
						<td>
							<%#= link_to content_tag(:i,nil, class: 'fa fa-pencil'), edit_admin_user_path(user), class: "btn btn-success btn-xs table-actions" %>
							<%#= link_to content_tag(:i,nil, class: 'fa fa-eye'), admin_user_path(user), class: "btn btn-info btn-xs table-actions" %>
						</td>
					</tr>
				<% end %>
			</table>
		</div>
	</div>	
</div>

